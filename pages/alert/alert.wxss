/* pages/alert/alert.wxss */
.wrap{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.75);
  position: fixed;
  left: 0;
  top: 0;
}
.innerText{
  color: #000;
}
.modals{
  margin: auto;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  left:0;
  width: 600rpx;
  height: 326rpx;
  background: #ffff;
  border-radius: 8rpx;
}
.innerText{
  text-align: center;
  padding: 82rpx 0 82rpx 0;
}
.btn{
  display: flex;
}
.btn button{
  display: inline-block;
  border-radius: 74rpx;
  padding: 0rpx 80rpx;
}
.sure{
  background: #CA191D;
  color: #fff;
}
.cancel{
  background: #fff;
  color: #CA191D;
  border: 1rpx solid #CA191D;
}